<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="415.0" prefWidth="583.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.editPLayerController">
   <children>
      <ImageView fitHeight="415.0" fitWidth="583.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@t20mngsys_bg.jpg" />
         </image>
      </ImageView>
      <Label layoutX="14.0" layoutY="14.0" text="Edit" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="33.0" text="Player Infomation" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="25.0" />
         </font>
      </Label>
      <Button fx:id="addPlayerBtn" layoutX="47.0" layoutY="134.0" mnemonicParsing="false" onAction="#setAddPlayerBtn" prefHeight="25.0" prefWidth="184.0" style="-fx-background-color: #fcba03#fcba03 #fcba03#fcba03;" text="Add Player">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="removePlayerBtn" layoutX="47.0" layoutY="170.0" mnemonicParsing="false" onAction="#setRemovePlayerBtn" prefHeight="25.0" prefWidth="184.0" style="-fx-background-color: #fcba03#fcba03 #fcba03#fcba03;" text="Remove Player">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="editPlayerBtn" layoutX="47.0" layoutY="206.0" mnemonicParsing="false" onAction="#setEditPlayerBtn" prefHeight="25.0" prefWidth="184.0" style="-fx-background-color: #fcba03#fcba03 #fcba03#fcba03;" text="Edit Player">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font>
      </Button>
      <Button id="quitBtn" fx:id="backBtn" layoutX="24.0" layoutY="334.0" mnemonicParsing="false" onAction="#setBackBtn" prefHeight="25.0" prefWidth="69.0" style="-fx-background-color: #03c2fc#03c2fc; -fx-background-radius: 90;" text="Back" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
